{
  "name": "@discord-archive/frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "React TypeScript frontend for Discord Archive System",
  "scripts": {
    "dev": "vite --port 3000",
    "prebuild": "cd ../shared && npm install && npm run build",
